      </Label>
      <Label layoutX="37.0" layoutY="283.0" text="Confirm Password :">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <PasswordField fx:id="txtPassword" layoutX="152.0" layoutY="248.0" />
      <PasswordField fx:id="txtRePassword" layoutX="211.0" layoutY="288.0" />
      <Button fx:id="btnSignup" layoutX="153.0" layoutY="415.0" mnemonicParsing="false" onAction="#btnSignupOnAction" prefHeight="30.0" prefWidth="103.0" style="-fx-background-color: #3750a1;" text="Sign Up" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="btnUpdate" layoutX="150.0" layoutY="476.0" mnemonicParsing="false" onAction="#btnUpdateOnAction" prefHeight="30.0" prefWidth="103.0" style="-fx-background-color: #3750a1;" text="Update" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
